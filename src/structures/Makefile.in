# Makefile for src/structures

SHELL = @SHELL@
top_srcdir = @top_srcdir@
srcdir = @srcdir@
VPATH = @srcdir@
depcomp = /bin/sh $(top_srcdir)/autotools/depcomp
CCDEPMODE = @CCDEPMODE@

@SET_MAKE@

CXX = @CXX@
LD = @CXX@
STRIP = @STRIP@
CXXFLAGS = @CXXFLAGS@ -I. -I$(top_srcdir) -I$(top_srcdir)/src @DEFS@ $(CFLGS)
CPPFLAGS = @CPPFLAGS@

OBJS = \
	hash_table.o

doofus:
	@echo ""
	@echo "Let's try this from the right directory..."
	@echo ""
	@cd ../.. && $(MAKE)

clean:
	@rm -f .depend *.o *~
	@rm -f *_test

test: test_binary_tree

test_binary_tree: binary_tree.h binary_tree_test.o
	$(LD) binary_tree_test.o -o binary_tree_test

structures: $(OBJS)

include ./.deps/includes

.SUFFIXES:
.SUFFIXES: .c .o .h

.c.o:
	@echo -e "Compiling: \033[1m`basename $< .c`\033[0m"
	source='$<' object='$@' depfile='.deps/$*.Po' tmpdepfile='.deps/$*.TPo' depmode=$(CCDEPMODE) $(depcomp) \
	$(CXX) $(CXXFLAGS) -I../.. -I$(top_srcdir) -I$(top_srcdir)/src $(CPPFLAGS) -c $< 


#safety hash
